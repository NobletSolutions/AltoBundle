{
  "name": "ns/alto-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides the a SOAP client for the ALTO Land Titles System",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Nathanael David Noblet",
      "email": "nathanael@noblet.ca"
    }
  ],
  "require": {
    "php": ">=7.1",
    "phpro/soap-client": "^0.7",
    "psr/log": "^1.0",
    "symfony/symfony": "^3.0",
    "symfony/serializer": "^3.0"
  },
  "require-dev": {
    "zendframework/zend-code": "^3.0.4",
    "phpunit/phpunit": "^5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "NS\\AltoBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NS\\AltoBundle\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "symfony-bin-dir": "bin"
  }
}
